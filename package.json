{
	"name": "webpack-template",
	"version": "0.0.1",
	"description": "webpack-template",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack serve --config ./config/dev.cfg.js",
		"build": "cross-env NODE_ENV=production webpack --progress --config ./config/prod.cfg.js && size-limit",
		"lint": "eslint src/*.js",
		"lint-fix": "eslint --fix src/*.js",
		"stylelint": "stylelint src/**/*.css",
		"stylelint-fix": "stylelint src/**/*.css --fix",
		"server": "node ./config/server/server.js",
		"sortmq": "node ./config/helpers/sortmq.js",
		"purify": "node ./config/helpers/purify.js",
		"size": "size-limit"
	},
	"size-limit": [
		{
			"path": "./dist/js/*.js",
			"limit": "2000 ms"
		}
	],
	"browserslist": {
		"production": [
			"last 2 versions",
			"not dead",
			"not ie 11",
			"not ie_mob 11",
			"not op_mini all",
			"not op_mob > 0",
			"not and_qq > 0",
			"not android > 0"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.14.8",
		"@babel/eslint-parser": "^7.14.7",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-env": "^7.14.8",
		"@babel/preset-react": "^7.14.5",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
		"@size-limit/preset-app": "^5.0.1",
		"@svgr/webpack": "^5.5.0",
		"autoprefixer": "^10.3.1",
		"babel-loader": "^8.2.2",
		"babel-plugin-lodash": "^3.3.4",
		"copy-webpack-plugin": "^9.0.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.2.0",
		"css-minimizer-webpack-plugin": "^3.0.2",
		"css-mqpacker": "^7.0.0",
		"dotenv-webpack": "^7.0.3",
		"eslint": "^7.31.0",
		"eslint-plugin-react": "^7.25.2",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-svg-import-helper": "^1.0.3",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^5.3.2",
		"image-webpack-loader": "^7.0.1",
		"lodash-webpack-plugin": "^0.11.6",
		"mini-css-extract-plugin": "^2.1.0",
		"node-static": "^0.7.11",
		"postcss": "^8.3.6",
		"postcss-loader": "^6.1.1",
		"purify-css": "^1.2.5",
		"react-refresh": "^0.10.0",
		"size-limit": "^5.0.1",
		"stylelint": "^13.13.1",
		"stylelint-config-standard": "^22.0.0",
		"svgo": "^2.3.1",
		"svgo-loader": "^3.0.0",
		"webpack": "^5.64.2",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.14.8",
		"clsx": "^1.1.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
